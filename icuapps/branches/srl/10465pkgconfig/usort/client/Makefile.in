## Makefile.in for ICU - example/usort/client
## Steven  R. Loomis

srcdir=@srcdir@
top_srcdir=@top_srcdir@

## Build directory information
top_builddir = ../..
subdir = usort/client

include $(top_srcdir)/icuapps.mk

## Target information
TARGET = usort

OBJECTS = usort_main.o

include $(top_srcdir)/usort/lib/usort.mk

DEPS = $(OBJECTS:.o=.d)

## List of phony targets
.PHONY : all install clean check

all: $(TARGET)

install:

clean:
	test -z "$(CLEANFILES)" || rm -f $(CLEANFILES)
	rm -f $(OBJECTS) $(TARGET)

distclean: clean
	rm -f Makefile $(DEPS)

check: $(TARGET)
	$(INVOKE) ./$(TARGET) < Makefile > Makefile-Sorted~

Makefile: $(srcdir)/Makefile.in  $(top_builddir)/config.status
	cd $(top_builddir) \g
	 && CONFIG_FILES=$(subdir)/$@ CONFIG_HEADERS= $(SHELL) ./config.status


$(TARGET) : $(OBJECTS)
	$(CC) -o $@ $(OBJECTS) $(LIBS)

ifneq ($(MAKECMDGOALS),distclean)
-include $(DEPS)
endif
