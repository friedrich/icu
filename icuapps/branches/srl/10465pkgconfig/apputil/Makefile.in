## Makefile.in for ICU - apputil
## Steven  R. Loomis, Feb 17, 2003

srcdir=@srcdir@
top_srcdir=@top_srcdir@
## Build directory information
top_builddir = ..
subdir = apputil
CONFIG_H=demo_config.h

LIBPREFIX=lib
# srcdir must be set before this is included
include $(top_builddir)/icuapps.mk

## Extra files to remove for 'make clean'
CLEANFILES = *~ $(DEPS)

INSTALL_DATA=data/*.gif data/*.html data/*.css
DATA_DIR=$(BINDIR)/data

include apputil.mk

all:

install:
	-mkdir -p $(DATA_DIR)
	cp -R $(INSTALL_DATA) $(DATA_DIR)

clean:
	test -z "$(CLEANFILES)" || rm -f $(CLEANFILES)
	rm -f $(OBJECTS)

distclean: clean
	rm -f Makefile $(DEPS) $(CONFIG_H)

check:

Makefile: $(srcdir)/Makefile.in  $(top_builddir)/config.status
	cd $(top_builddir) \
	 && CONFIG_FILES=$(subdir)/$@ CONFIG_HEADERS= $(SHELL) ./config.status

$(CONFIG_H): $(srcdir)/$(CONFIG_H).in  $(top_builddir)/config.status
	cd $(top_builddir) \
	 && CONFIG_FILES= CONFIG_HEADERS=$(subdir)/$@ $(SHELL) ./config.status


.PHONY: all install clean distclean check
-include local.mk

ifneq ($(MAKECMDGOALS),distclean)
-include $(DEPS)
endif
